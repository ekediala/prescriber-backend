{
  "name": "prescriber",
  "description": "A drug prescription web application",
  "repository": "https://github.com/ekediala/prescryber-backend.git",
  "scripts": {
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "cross-env NODE_ENV=production node src/main.js",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "prod": "",
    "watch": "cross-env NODE_ENV=development nodemon -r esm src/main.js",
    "test": "cross-env NODE_ENV=test echo 'Simulating Test...' && sleep 10 && echo 'All Tests Passed...' && exit 0"
  },
  "engines": {
    "node": "8",
    "npm": "6.14.2"
  },
  "keywords": [
    "Drug",
    "illness",
    "health",
    "prescription",
    "medicine"
  ],
  "author": "Eke Enyinnaya Diala",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-cache-controller": "^1.1.0",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.4",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.5"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6"
  },
  "private": true
}
